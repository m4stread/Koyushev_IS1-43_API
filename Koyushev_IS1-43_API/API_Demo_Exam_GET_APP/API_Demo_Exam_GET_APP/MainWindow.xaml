<Window x:Class="API_Demo_Exam_GET_APP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:API_Demo_Exam_GET_APP"
        mc:Ignorable="d"
        Title="Валидация данных" Height="200" Width="450">
    <Grid>
        <Button x:Name="GetFullName" Content="Получить данные" HorizontalAlignment="Left" Margin="16,19,0,0" VerticalAlignment="Top" Height="62" Width="148" Click="GetFullName_Click"/>
        <Button x:Name="SendTestResult" Content="Отправить результат теста" HorizontalAlignment="Left" Margin="16,100,0,0" VerticalAlignment="Top" Height="62" Width="148" Click="SendTestResult_click"/>
        <TextBlock x:Name="TextBoxFullName" HorizontalAlignment="Left" Margin="185,19,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="62" Width="244"/>
        <TextBlock x:Name="TextBoxResult" HorizontalAlignment="Left" Margin="185,100,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="62" Width="244"/>

    </Grid>
</Window>
